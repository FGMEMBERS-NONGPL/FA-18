 <!-- The altitude value -->
 <animation>
  <type>textranslate</type>
  <object-name>AltOnes</object-name>
  <property>position/altitude-ft</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>AltTens</object-name>
  <property>position/altitude-ft</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>AltHundreds</object-name>
  <property>position/altitude-ft</property>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>AltThousands</object-name>
  <property>position/altitude-ft</property>
  <factor>0.0001</factor>
  <step>1000</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>AltTenthousands</object-name>
  <property>position/altitude-ft</property>
  <factor>0.00001</factor>
  <step>10000</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <!-- Climb rate, is given in fps, displayed in fpm -->
 <animation>
  <type>textranslate</type>
  <object-name>ClimbOnes</object-name>
  <property>velocities/vertical-speed-fps[0]</property>
  <step>0.0166666</step>
  <interpolation>
   <!-- Interpolation table for 60*0.1*abs(...) -->
   <entry><ind>-1e6</ind><dep>6e6</dep></entry>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>1e6</ind><dep>6e6</dep></entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>ClimbTens</object-name>
  <property>velocities/vertical-speed-fps</property>
  <step>0.166666</step>
  <interpolation>
   <!-- Interpolation table for 60*0.01*abs(...) -->
   <entry><ind>-1e6</ind><dep>6e5</dep></entry>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>1e6</ind><dep>6e5</dep></entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>ClimbHundreds</object-name>
  <property>velocities/vertical-speed-fps</property>
  <step>1.66666</step>
  <interpolation>
   <!-- Interpolation table for 0.001*abs(...) -->
   <entry><ind>-1e6</ind><dep>6e4</dep></entry>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>1e6</ind><dep>6e4</dep></entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>ClimbThousands</object-name>
  <property>velocities/vertical-speed-fps</property>
  <step>16.6666</step>
  <interpolation>
   <!-- Interpolation table for 0.0001*abs(...) -->
   <entry><ind>-1e6</ind><dep>6e3</dep></entry>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>1e6</ind><dep>6e3</dep></entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>select</type>
  <object-name>ClimbSign</object-name>
  <condition>
   <less-than>
    <property>velocities/vertical-speed-fps</property>
    <value>0.0</value>
   </less-than>
  </condition>
 </animation>

