 <!-- Bracket -->
 <animation>
  <type>select</type>
  <object-name>Bracket</object-name>
  <condition>
   <greater-than>
    <property>controls/gear/gear-down</property>
    <value>10</value>
   </greater-than>
  </condition>
 </animation>

 <!-- ILS -->
 <animation>
  <type>select</type>
  <object-name>ILSVLine</object-name>
  <object-name>ILSHLine</object-name>
  <condition>
   <greater-than>
    <property>controls/gear/gear-down</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>
 <animation>
  <type>translate</type>
  <object-name>ILSVLine</object-name>
  <property>radios/nav/heading-needle-deflection</property>
  <interpolation>
   <entry>
    <ind>-10</ind>
    <dep>-0.075</dep>
   </entry>
   <entry>
    <ind>10</ind>
    <dep>0.075</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>translate</type>
  <object-name>ILSHLine</object-name>
  <property>radios/nav/gs-needle-deflection</property>
  <interpolation>
   <entry>
    <ind>-10.0</ind>
    <dep>-0.075</dep>
   </entry>
   <entry>
    <ind>10.0</ind>
    <dep>0.075</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <!-- Heading tape -->
 <animation>
  <type>select</type>
  <object-name>HeadingStripe</object-name>
  <condition>
   <greater-than>
    <property>xxx</property>
    <value>2</value>
   </greater-than>
  </condition>
 </animation>

 <!-- Waterline -->
  <animation>
  <type>select</type>
  <object-name>Waterline</object-name>
  <condition>
   <property>controls/gear/gear-down</property>
  </condition>
 </animation>

</PropertyList>
