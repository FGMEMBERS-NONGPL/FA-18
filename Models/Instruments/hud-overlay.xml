 <!-- AOA -->
 <animation>
  <type>select</type>
  <object-name>AlphaSign</object-name>
  <condition>
   <less-than>
    <property>orientation/alpha-deg</property>
    <value>0.0</value>
   </less-than>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>AlphaTens</object-name>
  <condition>
   <or>
   <greater-than-equals>
    <property>orientation/alpha-deg</property>
    <value>10.0</value>
   </greater-than-equals>
   <less-than-equals>
    <property>orientation/alpha-deg</property>
    <value>-10.0</value>
   </less-than-equals>
   </or>
  </condition>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>AlphaTens</object-name>
  <property>orientation/alpha-deg</property>
  <step>10</step>
  <interpolation>
   <!-- Interpolation table for 0.01*abs(...) -->
   <entry><ind>-1e6</ind><dep>1e4</dep></entry>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>1e6</ind><dep>1e4</dep></entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>AlphaOnes</object-name>
  <property>orientation/alpha-deg</property>
  <step>1</step>
  <interpolation>
   <!-- Interpolation table for 0.1*abs(...) -->
   <entry><ind>-1e6</ind><dep>1e5</dep></entry>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>1e6</ind><dep>1e5</dep></entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>textranslate</type>
  <object-name>AlphaTenths</object-name>
  <property>orientation/alpha-deg</property>
  <step>0.1</step>
  <interpolation>
   <!-- Interpolation table for abs(...) -->
   <entry><ind>-1e6</ind><dep>1e6</dep></entry>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>1e6</ind><dep>1e6</dep></entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

