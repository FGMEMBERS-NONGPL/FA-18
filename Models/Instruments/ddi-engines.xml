<?xml version="1.0"?>
<PropertyList>
 <path>ddi-engines.ac</path>

 <!-- The glass, it is included in the body. hide it here -->
 <animation>
  <type>select</type>
  <object-name>Display</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[2]/position-norm</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>

